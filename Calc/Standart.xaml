<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Name="WindowStyle" x:Key="winStyle" TargetType="Window">

        <Setter Property="Background" Value="White" />

    </Style>
    <Style x:Name="menuStyle" x:Key="mnStyle" TargetType="Menu">
        <Setter Property="Background" Value="White" />




    </Style>
    <Style x:Name="menuIt" x:Key="Header1" TargetType="Control">

        <Setter Property="BorderBrush" Value="Black"/>


    </Style>
    <Style x:Name="culIt" x:Key="culcControl" TargetType="Control">

        <Setter Property="BorderBrush" Value="Black"/>
        <Setter Property="FontSize" Value="20"/>


    </Style>
    <Style x:Name="radioStyle" x:Key="radioControl" TargetType="RadioButton">


    </Style>

</ResourceDictionary>